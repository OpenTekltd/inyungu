<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

  <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>vProducts</from-outcome>
            <to-view-id>/view_product.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>eProducts</from-outcome>
            <to-view-id>/edit_product.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>vStock</from-outcome>
            <to-view-id>/view_stock.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>eStock</from-outcome>
            <to-view-id>/edit_stock.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>vUser</from-outcome>
            <to-view-id>/view_user.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>eUser</from-outcome>
            <to-view-id>/edit_user.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>userMenu</from-outcome>
            <to-view-id>/user_dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>adminMenu</from-outcome>
            <to-view-id>/admin_dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>log</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>cProduct</from-outcome>
            <to-view-id>/create_product.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
    </navigation-rule>
  
    
    
</faces-config>
